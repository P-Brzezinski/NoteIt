    var ApiService = /*#__PURE__*/function () {
      function ApiService(http) {
        _classCallCheck(this, ApiService);

        this.http = http;
        this.BASE_URL = 'http://localhost:8082/api';
        this.ALL_NOTEBOOKS_URL = "".concat(this.BASE_URL, "\\notebooks\\all");
        this.SEND_FEEDBACK_URL = "".concat(this.BASE_URL, "\\feedback");
        this.SAVE_UPDATE_NOTEBOOK = "".concat(this.BASE_URL, "\\notebooks");
        this.DELETE_NOTEBOOK_URL = "".concat(this.BASE_URL, "\\notebooks\\");
        this.ALL_NOTES_URL = "".concat(this.BASE_URL, "\\notes\\all");
        this.NOTES_BY_NOTEBOOK_URL = "".concat(this.BASE_URL, "\\notes\\byNotebook\\");
        this.SAVE_UPDATE_NOTE_URL = "".concat(this.BASE_URL, "\\notes\\");
        this.DELETE_NOTE_URL = "".concat(this.BASE_URL, "\\notes\\");
      }

      _createClass(ApiService, [{
        key: "getAllNotebooks",
        value: function getAllNotebooks() {
          return this.http.get(this.ALL_NOTEBOOKS_URL);
        }
      }, {
        key: "getAllNotes",
        value: function getAllNotes() {
          return this.http.get(this.ALL_NOTES_URL);
        }
      }, {
        key: "getNotesByNotebook",
        value: function getNotesByNotebook(notebookId) {
          return this.http.get(this.NOTES_BY_NOTEBOOK_URL + notebookId);
        }
      }, {
        key: "postFeedback",
        value: function postFeedback(feedback) {
          return this.http.post(this.SEND_FEEDBACK_URL, feedback);
        }
      }, {
        key: "postNoteBook",
        value: function postNoteBook(notebook) {
          return this.http.post(this.SAVE_UPDATE_NOTEBOOK, notebook);
        }
      }, {
        key: "postNote",
        value: function postNote(note) {
          return this.http.post(this.SAVE_UPDATE_NOTE_URL, note);
        }
      }, {
        key: "deleteNotebook",
        value: function deleteNotebook(id) {
          return this.http["delete"](this.DELETE_NOTEBOOK_URL + id);
        }
      }, {
        key: "deleteNote",
        value: function deleteNote(notId) {
          return this.http["delete"](this.DELETE_NOTE_URL + notId);
        }
      }]);

      return ApiService;
    }();

    ApiService.ɵfac = function ApiService_Factory(t) {
      return new (t || ApiService)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵinject"](_angular_common_http__WEBPACK_IMPORTED_MODULE_1__["HttpClient"]));
